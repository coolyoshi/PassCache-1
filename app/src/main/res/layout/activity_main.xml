<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.android.passcache.Main">


  

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn9"
        android:background="@drawable/round_button"
        android:layout_row="4"
        android:layout_column="0"
        android:visibility="invisible"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/btn6" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn10"
        android:background="@drawable/round_button"
        android:layout_row="4"
        android:layout_column="2"
        android:visibility="invisible"
        android:layout_below="@+id/btn9"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn8"
        android:background="@drawable/round_button"
        android:layout_row="3"
        android:layout_column="2"
        android:visibility="invisible"
        android:layout_above="@+id/btn9"
        android:layout_toEndOf="@+id/btn10"
        android:layout_marginBottom="108dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn6"
        android:background="@drawable/round_button"
        android:layout_row="2"
        android:layout_column="2"
        android:visibility="invisible"
        android:layout_above="@+id/btn1"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn7"
        android:background="@drawable/round_button"
        android:layout_row="3"
        android:layout_column="0"
        android:visibility="invisible"
        android:layout_below="@+id/btn8"
        android:layout_alignStart="@+id/btn4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn5"
        android:background="@drawable/round_button"
        android:layout_row="2"
        android:layout_column="0"
        android:visibility="invisible"
        android:layout_above="@+id/btn7"
        android:layout_alignStart="@+id/btn8" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn3"
        android:background="@drawable/round_button"
        android:layout_row="1"
        android:layout_column="0"
        android:visibility="invisible"
        android:layout_above="@+id/btn9"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn1"
        android:background="@drawable/round_button"
        android:layout_row="0"
        android:layout_column="0"
        android:visibility="invisible"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/btn10" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn4"
        android:background="@drawable/round_button"
        android:layout_row="1"
        android:layout_column="2"
        android:visibility="invisible"
        android:layout_below="@+id/btn10"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/btn2"
        android:background="@drawable/round_button"
        android:layout_row="0"
        android:layout_column="2"
        android:visibility="invisible"
        android:layout_alignTop="@+id/btn8"
        android:layout_toStartOf="@+id/btn10" />


</RelativeLayout>
